{
    "builds": [
        {
            "src": "Cargo.toml",
            "use": "@vercel/rust"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
